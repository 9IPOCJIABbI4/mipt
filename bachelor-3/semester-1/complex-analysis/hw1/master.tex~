        % the default choice ’variable=\x’ leads to
        % unexpected results here!
 %       variable=\t,
        %quiver={
%u={-sin(deg(t))}, v={cos(deg(t))}, scale arrows=0.5,
  %      },
%	] ( {(2+t)sin(4*pi*deg(t))}, {(1+t)cos(4 * pi*deg(t))} );
    \addplot [color=tabblue,
        samples=1000, domain=0:1,
